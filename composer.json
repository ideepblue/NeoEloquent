{
    "name": "ideepblue/neoeloquent",
    "description": "Laravel wrapper for the Neo4j graph database REST interface",
    "keywords": [
        "laravel",
        "neo4j",
        "graph",
        "database",
        "neoeloquent"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        },
        {
            "name": "Ulobby",
            "email": "ulobby@ulobby.eu"
        },
        {
            "name": "Fan Dong",
            "email": "idongfan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0",
        "illuminate/database": "^6.0",
        "illuminate/pagination": "^6.0",
        "illuminate/events": "^6.0",
        "illuminate/console": "^6.0",
        "heydavid713/neo4jphp": "0.1.*",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.2.3",
        "mockery/mockery": "1.3.0",
        "codacy/coverage": "dev-master"
    },
    "repositories": [
    ],
    "autoload": {
        "psr-4": {
            "Vinelab\\NeoEloquent\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "replace": {
        "vinelab/neoeloquent":"1.4.7"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Vinelab\\NeoEloquent\\NeoEloquentServiceProvider"
            ]
        }
    }
}
